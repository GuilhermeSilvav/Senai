programa
{
	
	funcao inicio()
	{
		real num1, num2, num3, num4, num5, cesta
		

		escreva("Informe o numero1: ")
		leia(num1)
		
		escreva("Informe o numero2: ")
		leia(num2)

		escreva("Informe o numero3: ")
		leia(num3)
		
		escreva("Informe o numero4: ")
		leia(num4)

		escreva("Informe o numero5: ")
		leia(num5)
	
		cesta = num1

		se(num2 > cesta)
		cesta = num2

		se(num3 > cesta)
		num3 = cesta

		se(num4 > cesta)
		cesta = num4

		se(num5 > cesta)
		cesta = num5

		escreva("O maior numero Ã©:" + cesta)

	}							
}
